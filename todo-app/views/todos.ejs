<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Due Date</th>
      <th>Completed</th>
    </tr>
  </thead>
  <tbody>
    <% todos.forEach(todo => { %>
      <tr class="<%= todo.completed ? 'completed' : '' %>">
        <td><%= todo.id %></td>
        <td><%= todo.title %></td>
        <td><%= todo.dueDate.toISOString().slice(0, 10) %></td>
        <td><%= todo.completed ? "Yes" : "No" %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

